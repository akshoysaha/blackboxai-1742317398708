# Matrix Server Configuration
VITE_MATRIX_SERVER_URL=https://matrix.org
VITE_MATRIX_ACCESS_TOKEN=your_access_token_here
VITE_MATRIX_USER_ID=@username:matrix.org

# Application Configuration
VITE_APP_NAME=Talke
VITE_APP_DESCRIPTION=A modern Matrix chat client
VITE_APP_URL=http://localhost:3000

# Security Configuration
VITE_SESSION_SECRET=your_session_secret_here
VITE_ENCRYPTION_KEY=your_encryption_key_here

# Feature Flags
VITE_ENABLE_VOICE_CALLS=false
VITE_ENABLE_VIDEO_CALLS=false
VITE_ENABLE_FILE_SHARING=true
VITE_ENABLE_ENCRYPTION=true

# Development Settings
VITE_DEBUG_MODE=false
VITE_API_TIMEOUT=30000